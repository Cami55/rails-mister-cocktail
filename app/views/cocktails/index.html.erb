<%= render 'banner' %>
<div class="container">
  <div class="row">

    <% @cocktails.each do |cocktail| %>

      <div class="col-xs-12 col-sm-6 col-lg-4">
        <%= link_to cocktail_path(cocktail) do %>
              <h1><%= cocktail.name %></h1>

          </div>
        <% end %>
      </div>

    <% end %>

  </div>
</div>
